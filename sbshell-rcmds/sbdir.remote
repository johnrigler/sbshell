eval `xargs` # ; sbfont green ; env | grep SB ; echo $PATH ; sbfont white


THISSBPATH=$SBPATH
echo $1 | sed 's/\// /g' | xargs -n1 | while read SUBDIR
	do
	THISSBPATH="$THISSBPATH/$SUBDIR"
   	mkdir $THISSBPATH 2>/dev/null
	cd $THISSBPATH
   	ln -f -s $SBPATH/sbshell/index.php $THISSBPATH/index.php
	done
