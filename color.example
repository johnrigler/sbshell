#!/bin/bash

xcolor() { 
- color Changes the color in the shell or web version
- color Possible colors are white,bold,underline,error,red,green,yellow,blue,purple,aqua

	if [ -n $1 ]
	then
    case $1 in
        white)
            FG='00m'
        ;;
        red)
            FG='31m'
        ;;
        green)
            FG='32m'
        ;;
        blue)
            FG='34m'
        ;;
        purple)
            FG='35m'
        ;;
        aqua)
            FG='36m'
  			;;
    esac;

     printf "\033[$FG"
	fi

}
